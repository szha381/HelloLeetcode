class Solution(object):
    def lengthOfLongestSubstring(self, s):
        i = maxLen = 0
        dic = {}
        for j in range(len(s)):
            if s[j] in dic:
                i = max(i, dic[s[j]] + 1)
            dic[s[j]] = j
            maxLen = max(maxLen, j - i + 1)
        return maxLen
        
